{
  "presets": ["env", "react"],
  "plugins": [
    ["import", {"libraryName": "antd", "libraryDirectory": "es", "style": true}],
    "transform-runtime",
    "transform-class-properties",
    "transform-function-bind",
    "transform-object-rest-spread",
    "react-hot-loader/babel",
    "array-includes"
  ]
}
